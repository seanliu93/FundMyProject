<h1>Your profile</h1>
<br>
<h2>Available Balance ($): <%= @user.balance %></h2>
<h2>Your owned projects:</h2>
<% @user.projects.each do |p| %>
  <p><%= link_to p.title, project_path(p) %></p>
<% end %>
<h2>Your backed projects:</h2>
<% @user.backed_projects.each do |p| %>
  <p><%= link_to p.title, project_path(p) %></p>
<% end %>