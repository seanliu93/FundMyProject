<h1>Welcome to FundMyProject</h1>
<table class="table table-hover">
  <th>Project Name</th>
  <th>Amount Funded</th>
  <th>End Date</th>
  <% @projects.each do |p| %>
    <tr>
      <td><h2><%= link_to p.title, project_path(p)%></h2></td>
      <td><p><%= p.funded %> / <%= p.fund_goal%> ( <%= p.funded_percent %>%)</p></td>
      <td><p><%= p.end_date %></p>
    </tr>
  <% end %>
</table>